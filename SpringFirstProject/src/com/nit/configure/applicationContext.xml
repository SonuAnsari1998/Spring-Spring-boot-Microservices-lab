<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- dependent bean -->
    <bean id="ltime" class="java.time.LocalTime" factory-method="now"/>
    <bean id="ltime1" class="java.time.LocalDate" factory-method="now"/>

    <!-- target bean -->
    <bean id="wmg" class="com.nit.sbeans.WishMessageGenerator">
        <property name="time" ref="ltime"/>
        <property name="Date" ref="ltime1"/>
    </bean>

</beans>
